    "#số nhị phân\n",
    "def bin_strs(n):\n",
    "    if (n == 1) : return [\"0\", \"1\"]\n",
    "\n",
    "    A = []\n",
    "    for f in [\"0\", \"1\"]:\n",
    "        for s in bin_strs(n - 1):\n",
    "            A.append(f + s)\n",
    "    return A"
   ]
