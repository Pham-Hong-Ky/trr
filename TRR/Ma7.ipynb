    "def BubbleSort(x):\n",
    "    n = len(x)\n",
    "    for i in range(n - 1):\n",
    "        for j in range(n - 1, i, -1):\n",
    "            if (x[j] < x[j - 1]):\n",
    "                x[j - 1], x[j] = x[j], x[j - 1]\n",
    "    return x"
   ]
